{
    "name": "andrey-helldar/cashier-sber-auth",
    "description": "Sber API Authorization Driver",
    "type": "library",
    "license": "MIT",
    "keywords": [
        "andrey-helldar",
        "helldar",
        "bank",
        "billing",
        "cashier",
        "sber",
        "auth"
    ],
    "authors": [
        {
            "name": "Andrey Helldar",
            "email": "helldar@ai-rus.com"
        }
    ],
    "support": {
        "issues": "https://github.com/andrey-helldar/cashier-sber-auth/issues",
        "source": "https://github.com/andrey-helldar/cashier-sber-auth"
    },
    "require": {
        "php": "^7.2.5|^8.0",
        "ext-json": "*",
        "andrey-helldar/support": "^4.0",
        "illuminate/support": "^6.0|^7.0|^8.0",
        "nesbot/carbon": "^2.0",
        "psr/http-message": "^1.0",
        "ramsey/uuid": "^3.7|^4.0"
    },
    "require-dev": {
        "andrey-helldar/cashier": "dev-main",
        "symfony/var-dumper": "^5.0"
    },
    "autoload": {
        "psr-4": {
            "Helldar\\CashierDriver\\SberAuth\\": "src"
        }
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "repositories": [
        {
            "type": "path",
            "url": "../cashier"
        }
    ]
}
